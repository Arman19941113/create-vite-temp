    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^7.20.0",
    "postcss-html": "^1.3.0",
    "postcss-mixins": "^8.1.0",
